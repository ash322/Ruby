<style>
.jumbotron{
  margin-right: 15rem;
}

</style>
<div class="jumbotron">
  <%= form_for @paper do |f| %>

    <div class="form-group col-md-4 row">
      <%= f.label :Name %>
      <%= f.text_field :name, class: "form-control" %>
    </div>

    <div class = "actions" >
      <%= f.submit class: "btn btn-primary"%>
    </div >
  <% end %>
</div>
